				["init"] = {
					["custom"] = "--use brackets for npcIDs \"\"\naura_env.npcIDs = {\n    [\"149535\"] = true, -- Icebound Images\n    --[\"148907\"] = true, -- Jaina's Prismatic Images\n    --[\"87318\"] = true,\n}\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n--------------------------------------------------------------------\n\nlocal aura_env = aura_env\n\naura_env.stored = {}\naura_env.storedUIDs = {}\naura_env.lines = {}\n\naura_env.anchorPoint = {\n    [1] = \"BOTTOM\",\n    [2] = \"BOTTOMLEFT\",\n    [3] = \"BOTTOMRIGHT\",\n    [4] = \"CENTER\",\n    [5] = \"LEFT\",\n    [6] = \"RIGHT\",\n    [7] = \"TOP\",\n    [8] = \"TOPLEFT\",\n    [9] = \"TOPRIGHT\",\n}\n\nlocal createLine = function()\n    return UIParent:CreateLine(_, \"OVERLAY\", 7)\nend\nlocal releaseLine = function(_,line)\n    line:Hide()\nend\nif not aura_env.region.linePool then\n    aura_env.region.linePool = CreateObjectPool(createLine, releaseLine)\nend\n\nif not aura_env.region.hook then\n    hooksecurefunc(WeakAuras, \"OpenOptions\", function()\n            if not InCombatLockdown() then\n                aura_env.region.linePool:ReleaseAll()\n                aura_env.stored = {}\n                aura_env.lines = {}\n                aura_env.storedUIDs = {}\n            end\n    end)\nend\naura_env.region.hook = true",
